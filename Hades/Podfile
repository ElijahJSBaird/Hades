# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Hades' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Hades
  pod 'reddift', :git => 'https://github.com/sonsongithub/reddift.git', :branch => 'master'

  
  target 'HadesTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'HadesUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
